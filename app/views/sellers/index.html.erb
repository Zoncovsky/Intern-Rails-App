<h1>Sellers</h1>

<table class="table table-bordered table-hover">
    <thead class="table-dark" style="text-align: center;">
      <tr>
        <th>Seller Id</th>
        <th>Email</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @sellers.each do |seller| %>
        <tr style="text-align: center;">
          <td><%= seller.id %></td>
          <td><%= seller.email %></td>
      <% end %>
    </tbody>
  </table>