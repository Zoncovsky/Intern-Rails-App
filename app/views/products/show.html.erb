<div class="card w-50">
  <div class="card-body text-center">
    <h5 class="card-title">
    <strong>Name Product:</strong>
      <%= @product.name_product %>
    </h5>
    <h5 class="card-title">
    <strong>Quantity:</strong>
    <%= @product.quantity %>
    </h5>
  </div>
</div>

<%= link_to 'Edit', edit_product_path(@product), class:"btn btn-outline-success btn-sm" %> |
<%= link_to 'Back', products_path, class:"btn btn-outline-dark btn-sm" %>
